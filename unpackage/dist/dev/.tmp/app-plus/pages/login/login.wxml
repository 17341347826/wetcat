<form data-event-opts="{{[['submit',[['login',['$event']]]]]}}" class="loginView" bindsubmit="__e"><view class="dhl"></view><view><text class="loginLogo">WETCAT</text></view><view class="input-view"><input class="input" type="number" placeholder="  请输入手机号" name="nameValue" maxlength="11" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" value="{{phone}}" bindinput="__e"/></view><view class="pwdCode"><view class="input-view pwd"><input class="input" type="number" placeholder="  请输入验证码" name="passwordValue" maxlength="6"/></view><view><button class="code" type="primary" hover-class="hover" data-event-opts="{{[['tap',[['onGetCode']]]]}}" bindtap="__e">{{getCode?'获取验证码':countDown+'s'}}</button></view></view><view class="button-view"><button class="login" type="default" hover-class="hover" formType="submit">登录</button><view class="register" hover-class="hover" data-event-opts="{{[['tap',[['register']]]]}}" bindtap="__e"><text class="holdLogin">稍后登录</text></view></view><view style="width:100%;bottom:0;height:var(--status-bar-height);background:#36465D;position:fixed;"></view></form>