{
  "@platforms": [
    "android",
    "iPhone",
    "iPad"
  ],
  "id": "__UNI__7FA085B",
  "name": "kantu",
  "version": {
    "name": "1.0.2",
    "code": 102
  },
  "description": "图片App模版",
  "launch_path": "__uniappview.html",
  "developer": {
    "name": "",
    "email": "",
    "url": ""
  },
  "permissions": {
    "UniNView": {
      "description": "UniNView原生渲染"
    }
  },
  "plus": {
    "useragent": {
      "value": "uni-app",
      "concatenate": true
    },
    "splashscreen": {
      "target": "id:1",
      "autoclose": true,
      "waiting": true,
      "delay": 0
    },
    "popGesture": "close",
    "launchwebview": {
      "render": "always",
      "id": "1",
      "kernel": "WKWebview",
      "uni-app": "auto"
    },
    "statusbar": {
      "immersed": "supportedDevice",
      "style": "light",
      "background": "#36465D"
    },
    "usingComponents": true,
    "allowsInlineMediaPlayback": true,
    "distribute": {
      "plugins": {
        "audio": {
          "mp3": {
            "description": "Android平台录音支持MP3格式文件"
          }
        }
      }
    },
    "uni-app": {
      "compilerVersion": "2.3.7",
      "control": "v8",
      "nvueCompiler": "weex",
      "renderer": "auto"
    }
  }
}